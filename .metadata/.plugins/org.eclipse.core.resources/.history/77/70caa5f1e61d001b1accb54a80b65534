package com.example.demo.model;

import java.util.Date;

public class PostagemModel {
	
	private long id;
	
	private String titulo;
	
	private String texto;
	
	private Date date = new java.sql.Date(System.currentTimeMillis()); //para calcular exatamente a data que ele postou! se um dado passou por essa classe, vai calcular

	public long getId() {
		return id;
	}

	public void setId(long id) {
		this.id = id;
	}

	public String getTitulo() {
		return titulo;
	}

	public void setTitulo(String titulo) {
		this.titulo = titulo;
	}

	public String getTexto() {
		return texto;
	}

	public void setTexto(String texto) {
		this.texto = texto;
	}

	public Date getDate() {
		return date;
	}

	public void setDate(Date date) {
		this.date = date;
	}	
	
}
