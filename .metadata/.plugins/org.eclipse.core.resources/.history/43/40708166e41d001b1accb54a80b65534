#jpa faz varredura, faz comparação, está igual aos padrões da api? 
spring.jpa.hibernate.ddl-auto=update
#Conexao com o banco tipo IPServidor/NomeBanco/CriandoBanco, endereço
spring.datasource.url=jdbc:mysql://localhost/blog_pessoal?createDatabaseIfNotExist=true&serverTimezone=UTC&useSSl=false
#Inserção de usuario
spring.datasource.username=root
#inserção de senha
spring.datasource.password=
#para ver o sql no console
spring.jpa.show-sql=true
#para ver com mais clareza
spring.jpa.properties.hibernate.format_sql=true
